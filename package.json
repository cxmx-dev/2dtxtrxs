    "type": "git",
    "url": "https://github.com/yourusername/tetris-2d5.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/tetris-2d5/issues"
  },
  "homepage": "https://github.com/yourusername/tetris-2d5#readme",
  "devDependencies": {
    "http-server": "^14.1.1"
  },
  "engines": {
